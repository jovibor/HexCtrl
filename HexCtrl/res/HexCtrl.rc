IDR_HEXCTRL_MENU MENU
BEGIN
    POPUP ""
    BEGIN
        MENUITEM "Search and Replace\tCtrl+F",  IDM_HEXCTRL_SEARCH
        MENUITEM SEPARATOR
        POPUP "Show data as..."
        BEGIN
            MENUITEM "BYTE",                        IDM_HEXCTRL_SHOWAS_BYTE, CHECKED
            MENUITEM "WORD",                        IDM_HEXCTRL_SHOWAS_WORD
            MENUITEM "DWORD",                       IDM_HEXCTRL_SHOWAS_DWORD
            MENUITEM "QWORD",                       IDM_HEXCTRL_SHOWAS_QWORD
        END
        POPUP "Bookmarks"
        BEGIN
            MENUITEM "Add bookmark\tCtrl+B",        IDM_HEXCTRL_BOOKMARKS_ADD
            MENUITEM "Remove bookmark\tCtrl+N",     IDM_HEXCTRL_BOOKMARKS_REMOVE
            MENUITEM SEPARATOR
            MENUITEM "Go next bookmark\tF2",        IDM_HEXCTRL_BOOKMARKS_NEXT
            MENUITEM "Go previous bookmark\tShift+F2", IDM_HEXCTRL_BOOKMARKS_PREV
            MENUITEM SEPARATOR
            MENUITEM "Clear all bookmarks",         IDM_HEXCTRL_BOOKMARKS_CLEARALL
            MENUITEM SEPARATOR
            MENUITEM "Bookmarks manager",           IDM_HEXCTRL_BOOKMARKS_MANAGER
        END
        POPUP "Clipboard"
        BEGIN
            MENUITEM "Copy as Hex\tCtrl+C",         IDM_HEXCTRL_CLIPBOARD_COPYHEX
            MENUITEM "Copy as Hex (little-endian)", IDM_HEXCTRL_CLIPBOARD_COPYHEXLE
            MENUITEM "Copy as Hex Formatted",       IDM_HEXCTRL_CLIPBOARD_COPYHEXFMT
            MENUITEM SEPARATOR
            MENUITEM "Copy as Text",                IDM_HEXCTRL_CLIPBOARD_COPYTEXT
            MENUITEM "Copy as Base64",              IDM_HEXCTRL_CLIPBOARD_COPYBASE64
            MENUITEM "Copy as ""C"" Array",         IDM_HEXCTRL_CLIPBOARD_COPYCARRAY
            MENUITEM "Copy as Grep Hex",            IDM_HEXCTRL_CLIPBOARD_COPYGREPHEX
            MENUITEM "Copy as Print Screen",        IDM_HEXCTRL_CLIPBOARD_COPYPRINTSCREEN
            MENUITEM SEPARATOR
            MENUITEM "Paste as Hex\tCtrl+V",        IDM_HEXCTRL_CLIPBOARD_PASTEHEX
            MENUITEM "Paste as Text",               IDM_HEXCTRL_CLIPBOARD_PASTETEXT
        END
        POPUP "Modify"
        BEGIN
            MENUITEM "Operations...\tCtrl+O",       IDM_HEXCTRL_MODIFY_OPERS
            MENUITEM "Fill with zeros",             IDM_HEXCTRL_MODIFY_FILLZEROS
            MENUITEM "Fill with data...",           IDM_HEXCTRL_MODIFY_FILLDATA
            MENUITEM SEPARATOR
            MENUITEM "Undo\tCtrl+Z",                IDM_HEXCTRL_MODIFY_UNDO
            MENUITEM "Redo\tCtrl+Y",                IDM_HEXCTRL_MODIFY_REDO
        END
        POPUP "Selection"
        BEGIN
            MENUITEM "Mark Selection Start\tAlt+1", IDM_HEXCTRL_SEL_MARKSTART
            MENUITEM "Mark Selection End\tAlt+2",   IDM_HEXCTRL_SEL_MARKEND
            MENUITEM SEPARATOR
            MENUITEM "Select all\tCtrl+A",          IDM_HEXCTRL_SEL_ALL
        END
        MENUITEM "Data Interpreter",            IDM_HEXCTRL_DATAINTERPRET
        MENUITEM "Encoding",                    IDM_HEXCTRL_ENCODING
        POPUP "Appearance"
        BEGIN
            MENUITEM "Increase font size\tCtrl+MouseWheelUp", IDM_HEXCTRL_APPEARANCE_FONTINC
            MENUITEM "Decrease font size\tCtrl+MouseWheelDown", IDM_HEXCTRL_APPEARANCE_FONTDEC
            MENUITEM SEPARATOR
            MENUITEM "Increase capacity\tCtrl+Shift+MouseWheelUp", IDM_HEXCTRL_APPEARANCE_CAPACINC
            MENUITEM "Decrease capacity\tCtrl+Shift+MouseWheelDown", IDM_HEXCTRL_APPEARANCE_CAPACDEC
        END
        MENUITEM SEPARATOR
        MENUITEM "Print...",                    IDM_HEXCTRL_PRINT
        MENUITEM SEPARATOR
        MENUITEM "About",                       IDM_HEXCTRL_ABOUT
    END
END


